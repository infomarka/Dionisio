<h2>Cadastro de Usu√°rio</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :nome %><br />
  <%= f.text_field :nome %></div>
  
  <div><%= f.label :data_nascimento, "Data de Nascimento" %><br />
  <%= f.date_select :data_nascimento, :order => [:day, :month, :year], :start_year => data_inicio, :end_year => data_fim %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password, "Senha" %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation, "Confirme a Senha" %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Cadastrar" %></div>
<% end %>

<%= render :partial => "devise/shared/links" %>
